        "typescript": "^4.0.5"
    },
    "devDependencies": {
        "@types/express": "^4.17.8",
        "@types/jest": "^29.5.12",
        "@types/mongoose": "^5.7.36",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "supertest": "^7.0.0",
        "eslint": "^7.12.1",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.5"
    }
}
